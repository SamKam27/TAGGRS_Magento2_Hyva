{
    "name": "taggrs/magento2-hyva-data-layer",
    "description": "This extension makes the Taggrs_DataLayer extension compatible with the Hyva theme",
    "type": "magento2-module",
    "license": [
        "GPL-3.0-or-later"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "magento/framework": "*",
        "hyva-themes/magento2-compat-module-fallback": "*",
        "taggrs/magento2-data-layer": "*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Hyva\\TaggrsDataLayer\\": ""
        }
    }
}

